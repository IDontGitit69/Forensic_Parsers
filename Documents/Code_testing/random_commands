jq '.[] 
    | select(."winEventLog.id" == 4688) 
    | select((."winEventLog"."description" // "") | ascii_downcase | contains("zsatunnel")) 
    | {
        Time: ."winEventLog"."TimeCreated"."SystemTime",
        Creator: ."winEventLog"."EventData"."CreatorProcessName",
        NewProcess: ."winEventLog"."EventData"."NewProcessName",
        CommandLine: ."winEventLog"."EventData"."CommandLine",
        Description: ."winEventLog"."description"
      }' events.json
